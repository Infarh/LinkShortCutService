{
  "ConnectionStrings": {
    "Sqlite": "Data source=Link.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LinkOptions": {
    "HashStringLength": 5,
    "HashAlgorithm": "MD5",
    "Encoding": "UTF-32",
    "ConcurrentDbTimeout": 100,
    "ConcurrentDbTryCount": 10 
  },
  "AllowedHosts": "*"
}
